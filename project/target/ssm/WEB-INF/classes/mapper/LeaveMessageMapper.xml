<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="dao.LeaveMessageMapper">

    <insert id="saveLeaveMessage" parameterType="model.LeaveMessage">
        insert into leavemessage VALUES (#{messageId,jdbcType=INTEGER},#{userName,jdbcType=VARCHAR},#{phoneNumber,jdbcType=VARCHAR},#{qq,jdbcType=VARCHAR},#{message,jdbcType=VARCHAR});
    </insert>
</mapper>


